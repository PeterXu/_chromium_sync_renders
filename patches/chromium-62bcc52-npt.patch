+#include <stdlib.h>
+#include <stdio.h>
+
+#include "stream.h"
+#include "mp4.h"
+
+int main(int argc, char* argv[])
+{
+    if (argc != 2) {
+        printf("usage: %s mp4file\n", argv[0]);
+        exit(1);
+    }
+
+    mp4_box_t *root = NULL;
+    stream_t* s = NULL;
+
+    s = create_file_stream();
+    if (stream_open(s, argv[1], MODE_READ) == 0)
+        return -1;
