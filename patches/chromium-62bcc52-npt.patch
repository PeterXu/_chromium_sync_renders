+//  b) moof/
+
+// for some atom containers
+typedef unsigned char* data_t;
+typedef struct atom_info_s {
+    int     pos;
+    int     type;
+    uuid_t  uuid;
+    char    name[5]; // box name: 4chars + '\0'
+    int     size;
+    data_t  data;
+}atom_info_t;
+
+typedef struct mp4_root_s {
+    atom_info_t ftyp; // required
+    atom_info_t mmpu; // required
