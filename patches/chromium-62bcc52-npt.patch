+   uint8_t *traf_number;
+   uint8_t *trun_number;
+   uint8_t *sample_number;
+} atom_tfra_t;
+
+typedef struct atom_avcC_s {
+   uint8_t version;
+   uint8_t profile;
+   uint8_t profile_compatibility;
