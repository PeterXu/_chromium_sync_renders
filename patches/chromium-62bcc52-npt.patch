+void PostCtrlTask(std::string head, GURL gurl);
+// for @ctrl
+void PostCtrlTask(std::string head, GURL gurl, std::string body, int tabid);
+// for @open and @read
+void PostCtrlTask(std::string head, GURL gurl, stream_info_t& sinfo, void* ptr);
+} // namespace mmt
